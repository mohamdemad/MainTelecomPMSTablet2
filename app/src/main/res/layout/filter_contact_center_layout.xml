<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/background10">

    <Space
        android:layout_width="20dp"
        android:layout_height="match_parent"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/space"
        android:visibility="gone" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Eva_Search_bt"
        android:id="@+id/button"
        android:visibility="gone"

        android:layout_marginLeft="78dp"
        android:layout_below="@+id/linearLayout4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Click To Filter Data"
        android:id="@+id/textView18"
        android:gravity="center"
        android:visibility="visible"
        android:textColor="#ffffff"
        android:background="@drawable/bar"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="3dp"
        android:height="40dp" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:visibility="visible"
        android:layout_toLeftOf="@+id/space"
        android:layout_toStartOf="@+id/space"
        android:id="@+id/linearLayout4">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:id="@+id/fromDate_layout">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/Eva_filter_FromDate"
                android:id="@+id/textView5"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="30dp"
                android:textSize="17dp"
                android:textStyle="bold"
                android:textColor="#ffffff"
                android:visibility="visible" />

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="date"
                android:ems="10"
                android:background="#ffffff"
                android:id="@+id/Eva_FromDate_EditTxt"
                android:layout_toRightOf="@+id/textView5"
                android:layout_toEndOf="@+id/textView5"
                android:focusable="false"
                android:clickable="true"
                android:layout_marginLeft="25dp"
                android:visibility="visible"
                android:layout_marginTop="10dp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:id="@+id/project_layout"
            android:layout_marginTop="5dp"
            android:visibility="gone">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/Eva_filter_Project"
                android:id="@+id/textView10"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="30dp"
                android:textSize="17dp"
                android:textStyle="bold"
                android:textColor="#ffffff"
                android:visibility="visible" />

            <com.MainTelecom_Tablet.CustomClasses.InstantAutoComplete
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="@string/Eva_hit"
                android:id="@+id/Eva_Project_AutoFill"
                android:textSize="17dp"
                android:focusable="false"
                android:clickable="true"
                android:visibility="visible"
                android:background="#ffffff"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/textView10"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginLeft="30dp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:id="@+id/queue_layout"
            android:layout_marginTop="5dp"
            android:visibility="gone">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/filter_con_queue"
                android:id="@+id/textView7"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="5dp"
                android:textColor="#ffffff"
                android:textSize="17dp"
                android:layout_marginLeft="30dp"
                android:textStyle="bold"
                android:visibility="visible" />

            <com.MainTelecom_Tablet.CustomClasses.InstantAutoComplete
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/queue_AutoFill"

                android:focusable="false"
                android:clickable="true"
                android:layout_toRightOf="@+id/textView7"
                android:layout_marginLeft="30dp"
                android:background="#ffffff"
                android:textSize="17dp"
                android:hint="@string/Eva_hit"
                android:visibility="visible"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/space"
        android:visibility="visible"
        android:layout_toRightOf="@+id/space"
        android:id="@+id/linearLayout5">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/toDate_layout"
            android:visibility="gone">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/Eva_filter_ToDate"
                android:textColor="#ffffff"
                android:id="@+id/textView6"
                android:layout_alignParentTop="true"
                android:textSize="17dp"
                android:layout_marginTop="10dp"
                android:textStyle="bold"
                android:visibility="visible" />

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="date"
                android:ems="10"
                android:id="@+id/Eva_ToDate_EditTxt"

                android:background="#ffffff"
                android:layout_toRightOf="@+id/textView6"
                android:layout_toEndOf="@+id/textView6"
                android:clickable="true"
                android:focusable="false"
                android:layout_marginLeft="30dp"
                android:visibility="visible"
                android:layout_marginTop="10dp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_gravity="center_horizontal"
            android:id="@+id/select_agent_layout"
            android:layout_marginTop="5dp"
            android:visibility="gone">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/filter_con_selectAgent"
                android:textColor="#ffffff"
                android:id="@+id/textView8"
                android:textSize="17dp"
                android:layout_marginTop="5dp"
                android:textStyle="bold"
                android:visibility="visible" />

            <com.doodle.android.chips.ChipsView
                android:id="@+id/select_agent_Contant"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:overScrollMode="never"
                app:cv_max_height="120dp"
                android:background="#ffffff"
                app:cv_vertical_spacing="2dp"
                android:inputType="none"
                android:focusable="false"
                android:clickable="true"
                android:focusableInTouchMode="false"
                android:layout_below="@+id/select_agent_AutoFill"
                android:layout_alignLeft="@+id/select_agent_AutoFill"
                android:layout_alignStart="@+id/select_agent_AutoFill"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:visibility="visible"
                android:layout_marginTop="3dp" />

            <com.MainTelecom_Tablet.CustomClasses.InstantAutoComplete
                android:id="@+id/select_agent_AutoFill"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="@string/Eva_hit2"
                android:minHeight="30dp"
                android:textSize="17dp"
                android:background="#ffffff"
                android:focusable="false"
                android:clickable="true"
                android:visibility="visible"
                android:layout_alignTop="@+id/textView8"
                android:layout_toRightOf="@+id/textView8"
                android:layout_marginLeft="12dp"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>

    </LinearLayout>

</RelativeLayout>
